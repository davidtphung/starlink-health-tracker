{
  "name": "starlink-health-tracker",
  "version": "1.0.0",
  "description": "Live Starlink satellite health tracker with 3D globe visualization",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "vite build",
    "build:server": "esbuild server/index.ts --bundle --platform=node --format=esm --outfile=dist/server.js --external:express --external:cors --external:node-fetch",
    "start": "NODE_ENV=production node dist/server.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^8.15.0",
    "@tanstack/react-query": "^5.60.5",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "express": "^4.21.2",
    "framer-motion": "^11.13.1",
    "lucide-react": "^0.453.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.0",
    "satellite.js": "^5.0.0",
    "tailwind-merge": "^2.5.4",
    "three": "^0.160.0",
    "wouter": "^3.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.160.0",
    "@vercel/node": "^5.6.3",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.25.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.14"
  }
